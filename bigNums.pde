int[][]mNumI(int len, int len2)
{
  int[][]myNum=new int[len][len2];
  for(int i=0; i<len; i++)
  {
    for(int j=0; j<len2; j++)
    {
      myNum[i][j]=(int)(random(1e7));
    }
  }
  return myNum;
}
